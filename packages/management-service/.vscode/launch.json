  "version": "0.2.0",
  "configurations": [
    {
        "type": "node",
        "request": "launch",
        "name": "Debug Management service",
        "args": [
            "${workspaceFolder}/src/main.ts"
        ],
        "runtimeExecutable": "npm",
        "runtimeArgs": [
            "run-script", "start:dev"
        ],
        "sourceMaps": true,
        "envFile": "${workspaceFolder}/../../.env",
        "cwd": "${workspaceRoot}",
        "console": "integratedTerminal",
        "protocol": "inspector"
    }
]
}